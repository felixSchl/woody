{
  "name": "woody",
  "version": "0.1.2",
  "description": "Tiny logging combinator library for node",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run test:node && npm run test:browser",
    "test:node": "mocha --require source-map-support/register --reporter=list --compilers js:babel/register",
    "test:browser": "./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "build": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixSchl/woody.git"
  },
  "keywords": [
    "logging"
  ],
  "author": "Felix Schlitter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felixSchl/woody/issues"
  },
  "homepage": "https://github.com/felixSchl/woody",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^3.7.0",
    "moment": "^2.10.2",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "mocha": "^2.1.0",
    "phantomjs": "^1.9.18",
    "source-map-support": "^0.3.2",
    "vinyl-source-stream": "^1.1.0"
  }
}
